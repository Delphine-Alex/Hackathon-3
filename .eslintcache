;\n    } else if (direction.up) {\n      setProductIndex(productIndex - 1);\n    }\n    setDirection(directionInit);\n  };\n\n  useEffect(() => {\n    const wheelScroll = (e) => {\n      if (e.deltaY > 0) {\n        if (productIndex < data.length - 1) {\n          setDirection({ ...direction, down: true, center: true });\n          setMainDirection(\"down\");\n        }\n      } else {\n        if (productIndex > 0) {\n          setDirection({ ...direction, up: true, center: true });\n          setMainDirection(\"up\");\n        }\n      }\n    };\n\n    window.addEventListener(\"wheel\", wheelScroll);\n\n    return () => {\n      window.removeEventListener(\"wheel\", wheelScroll);\n    };\n  }, [productIndex, data.length, direction]);\n\n  useEffect(() => {\n    if (match.url) {\n      const cat = match.url.split(\"/\")[1];\n      const prod = match.url.split(\"/\")[2];\n      setSeo({ ...seo, categorie: cat, product: prod });\n    }\n  }, [match]);\n\n  return (\n    <main>\n      <Helmet>\n        <title>\n          Ichi Ni San\n          {\" \" +\n            seo.product +\n            ` ${data[productIndex] && data[productIndex].name}`}\n        </title>\n        <meta\n          name=\"description\"\n          content={`Japanese ${\n            seo.product && seo.product\n          } imported by ichi ni san from japan ! `}\n        />\n        <meta\n          name=\"keywords\"\n          content=\"japan , product , candies , products , imported , kawai\"\n        />\n        <meta name=\"robots\" content=\"index,follow\" />\n        <meta name=\"author\" content=\"un dos tres\"></meta>\n        <meta name=\"url\" content=\"http://www.ichinisan.com\"></meta>\n        <meta name=\"coverage\" content=\"Worldwide\"></meta>\n        <meta name=\"publisher\" content=\"WCS\"></meta>\n        <link rel=\"canonical\" href=\"https://www.ichinisan.com/\" />\n      </Helmet>\n      <div>\n        <section\n          className=\"upside\"\n          style={{\n            background: `${data[productIndex] && data[productIndex].gradient}`,\n          }}\n        >\n          <h1 className=\"title\">Ichi Ni San</h1>\n\n          <article className=\"product-card-container\">\n            <ProductInfos data={data} productIndex={productIndex} />\n            <AddToBox item={data[productIndex]} />\n          </article>\n\n          <ImageCenter\n            data={data}\n            productIndex={productIndex}\n            direction={direction}\n            changeIndex={changeIndex}\n            mainDirection={mainDirection}\n          />\n\n          <Nav handleState={handleState} />\n          <Burger handleState={handleState} />\n        </section>\n\n        {state && <SlideCart handleState={handleState} />}\n      </div>\n      <div>\n        <footer\n          className=\"downside\"\n          style={{\n            background: `url(\"/${\n              data[productIndex] && data[productIndex].background\n            }\")`,\n          }}\n        >\n          <NavArticle changeProduct={changeProduct} />\n        </footer>\n      </div>\n    </main>\n  );\n};\n\nexport default ProductCard;\n","/Users/jeromenicco/workspace/Hackathon-3/src/components/Shop/SlideCart.js",[],"/Users/jeromenicco/workspace/Hackathon-3/src/customHooks/useRequest.js",[],"/Users/jeromenicco/workspace/Hackathon-3/src/components/AddToBox.jsx",[],"/Users/jeromenicco/workspace/Hackathon-3/src/components/ProductQuantity.jsx",[],"/Users/jeromenicco/workspace/Hackathon-3/src/components/Panier/Panier.jsx",[],"/Users/jeromenicco/workspace/Hackathon-3/src/components/Panier/PanierCard.jsx",[],"/Users/jeromenicco/workspace/Hackathon-3/src/components/ImageCenter.jsx",[],"/Users/jeromenicco/workspace/Hackathon-3/src/components/ProductInfos.jsx",[],"/Users/jeromenicco/workspace/Hackathon-3/src/components/Nav.js",[],"/Users/jeromenicco/workspace/Hackathon-3/src/components/NavArticle.js",[],"/Users/jeromenicco/workspace/Hackathon-3/src/components/Burger.jsx",[],{"ruleId":"67","re